<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean destroy-method="stop" init-method="start" id="bridge3"
          class="org.apache.activemq.artemis.jms.bridge.impl.JMSBridgeImpl">
        <property name="bridgeName" value="fromMqToArtemis01"/>
        <property name="sourceConnectionFactoryFactory" ref="mqFactoryFactory"/>
        <property name="sourceDestinationFactory">
            <bean class="ch.dulce.springjmsbridge.factory.MQDestinationFactory">
                <property name="destinationName" value="DEV.QUEUE.1"/>
            </bean>
        </property>
        <property name="targetConnectionFactoryFactory" ref="artemisFactoryFactory"/>
        <property name="targetDestinationFactory">
            <bean class="ch.dulce.springjmsbridge.factory.ArtemisDestinationFactory">
                <property name="destinationName" value="to.queue.01"/>
            </bean>
        </property>
        <property name="failureRetryInterval" value="5000"/>
        <property name="maxBatchSize" value="10"/>
        <property name="maxBatchTime" value="5000"/>
        <property name="maxRetries" value="-1"/>
        <property name="qualityOfServiceMode" value="DUPLICATES_OK"/>
    </bean>

</beans>